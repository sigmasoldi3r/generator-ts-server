{
  "name": "<%= name %>",
  "private": <%= isPrivate %>,
  "version": "0.0.1",
  "description": "<%= description %>",
  "license": "<%= license %>",
  "scripts": {
    "start": "npm run build && node ./build/app.js",
    "start:watch": "npm run build:watch & nodemon ./build/app.js",
    "build": "node_modules/.bin/tsc",
    "build:watch": "npm run build -- -w"
  },
  <% if (author) { %>
  "author": {
    "name": "<%= author %>",
    <% if (email) { %>
    "email": "<%= email %>"
    <% } %>
  },
  <% } %>
  <% if (repo) { %>
  "repository": {
    "type": "git",
    "url": "https://github.com/<%= repo %>.git"
  },
  "bugs": {
    "url": "https://github.com/<%= repo %>/issues"
  },
  <% } %>
  "tags": [
    "express",
    "typescript-express",
    "routing-controllers"
  ],
  "dependencies": {
    "body-parser": "1.17.2",
    "express": "4.15.3",
    "js-yaml": "^3.10.0",
    "morgan": "^1.9.0",
    "multer": "1.3.0",
    "reflect-metadata": "0.1.10",
    "routing-controllers": "0.7.0",
    "twig": "^1.10.5",
    "typedi": "0.5.2",
    "typeorm": "^0.1.9",
    "typeorm-routing-controllers-extensions": "^0.1.0",
    "typeorm-typedi-extensions": "^0.1.1"
  },
  "devDependencies": {
    "@types/body-parser": "1.16.3",
    "@types/express": "4.0.35",
    "@types/js-yaml": "^3.10.1",
    "@types/morgan": "^1.7.35",
    "@types/multer": "0.0.33",
    "@types/node": "7.0.22",
    "@types/twig": "^1.10.0",
    "nodemon": "1.11.0",
    "typescript": "2.3.3"
  }
}
